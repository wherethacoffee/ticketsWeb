<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Tickets web</title>
        <link rel="stylesheet" href="https://bootswatch.com/5/flatly/bootstrap.min.css">
    </head>
    <body>
        <nav class="navbar navbar-dark bg-dark">
            <a href="../" class="navbar-brand">Gestor admin</a>
        </nav>
<div class="container mt-5">
    <div class="row">
        <div class="col-md-5 offset-md-3 card bg-dark">
            <div class="card-body">
                <form action="/CRUD_admin/update/<%= data.curp %>" method="post">
                    <input
                    type="text"
                    name="curp"
                    value="<%= data.curp %>"
                    placeholder="CURP"
                    class="form-control mb-3"
                    autofocus
                    />
                    <input
                    type="text"
                    name="nombre"
                    value="<%= data.nombre %>"
                    placeholder="Nombre"
                    class="form-control mb-3"
                    />
                    <input
                    type="text"
                    name="paterno"
                    value="<%= data.paterno %>"
                    placeholder="Paterno"
                    class="form-control mb-3"
                    autofocus
                    />
                    <input
                    type="text"
                    name="materno"
                    value="<%= data.materno %>"
                    placeholder="Materno"
                    class="form-control mb-3"
                    autofocus
                    />
                    <input
                    type="text"
                    name="telefono"
                    value="<%= data.telefono %>"
                    placeholder="Telefono"
                    class="form-control mb-3"
                    autofocus
                    />
                    <input
                    type="text"
                    name="correo"
                    value="<%= data.correo %>"
                    placeholder="Correo"
                    class="form-control mb-3"
                    autofocus
                    />
                    <input
                    type="text"
                    name="nivel"
                    value="<%= data.nivel %>"
                    placeholder="Nivel"
                    class="form-control mb-3"
                    autofocus
                    />
                    <label for="nivel" class="form-label mt-4">Nivel</label>
                        <select class="form-select" name="nivel" id="nivel">
                            <option>Seleccione</option>
                            <option>Primaria</option>
                            <option>Secundaria</option>
                        </select>
                    <label for="asunto" class="form-label mt-4">Asunto</label>
                        <select class="form-select" name="asunto" id="asunto">
                            <option>Seleccione</option>
                            <option>Entrega de papeleria</option>
                            <option>solicitud de cita</option>
                        </select>
                    <label for="asunto" class="form-label mt-4">Asunto</label>
                        <select class="form-select" name="asunto" id="asunto">
                            <option>Seleccione</option>
                            <option>Entrega de papeleria</option>
                            <option>solicitud de cita</option>
                        </select>
                    <button type="submit" class="btn btn-primary">Guardar Usuario</button>
                </form>
            </div>
        </div>
    </div>
</div>
</body>
</html>